{
  "scripts": {
    "dev": "yarn docker:up",
    "down": "yarn docker:down",
    "docker:up": "docker-compose -f docker-compose.local.yml up",
    "docker:down": "docker-compose -f docker-compose.local.yml down",
    "commit": "cz"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.8",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "3.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}

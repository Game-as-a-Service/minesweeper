{
  "extends": "@vue/tsconfig/tsconfig.web.json",
//  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "../lib/src/**/*"],
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/lib/*": ["../lib/src/*"],
      "@/*": ["./src/*"],
    },
    "strict": true,
  }
}
